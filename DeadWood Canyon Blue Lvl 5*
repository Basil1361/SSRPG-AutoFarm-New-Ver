func DeadwoodWhiteLvl5()
  ? loc = Deadwood    
    ? summon.count = 0 & foe.count = 0
    ^& item.GetCooldown(＂cinderwisp＂) <= 0
    ^& item.CanActivate()
      equipL fire_talisman
      activate L

    :? foe.count >= 0 
    ^& pickup.distance <= 10
      equipL Star
      equipR Tri

    :? foe = Huge Mosquito | foe = Stone Scarab
      ? foe.count = 0 
         equip Dashing Shield
         equipL Triske 

      :? foe.count >= 20
      ^&item.GetCooldown(＂blade＂) <= 0
        equip blade 
        activate blade

      :? foe.count > 0 & foe.distance >= 10
         activate potion
         equip Dashing Shield
         equipL Triske
      : 
        equipL vigor sword
        equipR Poison Hammer

    :? foe = Xylo
      ? foe.distance >= 10
        equip Dashing Shield
        equipL Triske
      :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
      :?item.GetCooldown(＂bardiche＂) <= 0 
      ^|(item.right = ＂bardiche＂
      ^& item.right.state = 2)
        equip bardiche
        activate R

      : 
        equipL Vigor Sword
        equipR Fire Sword

    :? foe = ＂Poena＂
      ? foe.buffs.string = ＂poena_mirror＂
      ^& item.GetCooldown(＂cinderwisp＂) <= 0
      ^& item.CanActivate()
        equipR fire_talisman
        activate R

        ? summon.count = 0
        ^& foe.buffs.string = ＂poena_mirror＂
          equip bar + 11

      :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
      :?item.GetCooldown(＂bardiche＂)  <= 0 
      ^|(item.right = ＂bardiche＂
      ^& item.right.state = 2)
        equip bardiche
        activate R
     :
       equipL vigor sword *9 + 13
       equipR poison hammer * 9 + 11

    : 
      equip Dashing Shield
      equipL Tri
