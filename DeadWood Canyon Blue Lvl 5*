func DeadwoodBlueLvl5()
  ? loc = Deadwood  
    ? hp < 7
      activate potion
    ? foe = ＂Poena＂
      ?foe.distance >= 7
         equip dashing shield
      :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
     :?item.GetCooldown(＂bardiche＂)  <= 0 
     ^|(item.right = ＂bardiche＂
     ^& item.right.state = 2)
       equip bardiche
       activate R
     :
       equipL vigor sword *7 + 13
       equipR poison hammer * 8 + 11

   ?foe = ＂Xylo＂  
     ?foe.distance >= 7
       equip dashing shield
     :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
     :?item.GetCooldown(＂bardiche＂)  <= 0 
     ^|(item.right = ＂bardiche＂
     ^& item.right.state = 2)
       equip bardiche
       activate R
     :
       equipL vigor sword *7 + 13
       equipR poison hammer * 8 + 11
      

    :? foe ! boss
      ? foe.count >= 30
      ^&item.GetCooldown(＂blade＂) <= 0
        equip blade 
        activate blade
      :
        equipL Vigor Sword
        equipR Poison War Hammer

    
    ? harvest.distance < 5
      equip hatchet
      activate hatchet

    ? pickup.distance < 10
      equip star

 DeadwoodBlueLvl5()
