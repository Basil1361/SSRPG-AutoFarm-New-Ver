func RockyGoldLvl1()
  ? loc = Rocky  
    ? hp < 7
      activate potion
    ? foe = ＂Acronian Scout＂
      ?foe.distance >= 7
         equip dashing shield
      :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
     :?item.GetCooldown(＂bardiche＂)  <= 0 
     ^|(item.right = ＂bardiche＂
     ^& item.right.state = 2)
       equip bardiche
       activate R
     :
       equipL vigor sword *7 + 13
       equipR poison hammer * 8 + 11

    :? foe = ＂phase1＂
      ?foe.distance >= 7
         equip dashing shield
      :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
     :?item.GetCooldown(＂bardiche＂)  <= 0 
     ^|(item.right = ＂bardiche＂
     ^& item.right.state = 2)
       equip bardiche
       activate R
     :
       equipL vigor sword *7 + 13
       equipR poison hammer * 8 + 11

    :? foe = ＂phase2＂
      ?foe.distance >= 7
         equip dashing shield
      :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
     :?item.GetCooldown(＂bardiche＂)  <= 0 
     ^|(item.right = ＂bardiche＂
     ^& item.right.state = 2)
       equip bardiche
       activate R

      :?foe = ＂fire＂
          equipL Vigor Sword
          equipR ether wand
      :?foe = ＂ether＂
          equipL Vigor Sword
          equipR Poison WarHammer
      :?foe = ＂ice＂
          equipL vigor Sword
          equipR Fire Hammer
      :?foe = ＂vigor＂
          equipL Vigor Sword  
          equipR Poison Hammer
      :?foe = ＂poison＂
          equipL Vigor Sword
          equipR Ice Wand
    :? foe = ＂phase3＂
      ?foe.distance >= 7
        equip bashing shield
      :? foe.state = 115 
      ^& foe.time >= 65
      ^&item.GetCooldown(＂mind＂) <= 0 
        equipL mind Stone
      :
       equipL = vigor sword
       equipR = Poison Hammer

    ? harvest.distance < 5
      equip shovel
      activate shovel


    ? pickup.distance < 10
      equip star

RockyGoldLvl1()
