func CavesBlueLvl5()
  ? loc = Caves 
    ? hp < 7
      activate potion
    ? foe = boss & hp > 0
      ?foe.state = 133
      ^&item.GetCooldown(＂mind＂) <= 0 
        equipL mind Stone 
      : 
        equipL ice wand
        equipR ether wand
                
    :? foe ! boss
      ? foe.count >= 30
      ^&item.GetCooldown(＂blade＂) <= 0
        equip blade 
        activate blade
      :
        equipL Vigor Sword
        equipR Poison War Hammer

CavesBlueLvl5()
