
func CavesBlueLvl5()
  ? loc = Caves 
    ? summon.count = 0 & foe.count = 0
    ^& item.GetCooldown(＂cinderwisp＂) <= 0
    ^& item.CanActivate()
      equipL fire_talisman
      activate L

    :? foe.count >= 0 
    ^& pickup.distance <= 10
      equipL Star
      equipR Tri

    :? foe = Neck Warmer | foe = Skin Peeler
    ^| foe = Wound Licker | foe = Nail Biter
    ^| foe = Knee Chopper | foe = Not Caviar 
      ? foe.count >= 20
      ^&item.GetCooldown(＂blade＂) <= 0
        equip blade 
        activate blade

      ? foe.distance <= 25
        ? hp = maxhp
          equip ice bow

        :
          equipL Vigor Wand
          equipR Ether Wand
      :
        equipL Tri
        equipR Shield *9

    :? foe = Bolesh
      ?foe.debuffs.GetCount(＂debuff_chill＂) 
      ^ = 6 
      ^& foe.state ! 133
        equipL Ether Wand
        equipR Vigor Wand 
  
      :?foe.state = 133
      ^&item.GetCooldown(＂mind＂) <= 0 
        equipL mind Stone
        equipR tri
      :
        equip ice bow

    :? foe = Ceiling Decorator
      ?foe.distance >= 7
        equipL Vigor Sword
        equip dashing shield
      :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
     :?item.GetCooldown(＂bardiche＂)  <= 0 
     ^|(item.right = ＂bardiche＂
     ^& item.right.state = 2)
       equip bardiche
       activate R
     :
       equipL vigor sword 
       equipR poison hammer      
                
    :
      equip shield
      equipL tri
