func RockyGoldLvl1()
  ? loc = Rocky  
    ? hp < 7
      activate potion
    ? summon.count = 0 & foe.count = 0
    ^& item.GetCooldown(＂cinderwisp＂) <= 0
    ^& item.CanActivate()
      equipL fire_talisman
      activate L

    :? foe.count >= 0 
    ^& pickup.distance <= 10
      equipL Star
      equipR Tri

    :? foe = ＂Acronian Scout＂
      ?foe.distance >= 7
         equipL tri
         equip dashing shield
      :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
     :?item.GetCooldown(＂bardiche＂)  <= 0 
     ^|(item.right = ＂bardiche＂
     ^& item.right.state = 2)
       equip bardiche
       activate R
     :
       equipL vigor sword *7 + 13
       equipR poison hammer * 8 + 11

    :? foe = ＂phase1＂
      ?foe.distance >= 7
         equipL tri
         equip dashing shield
      :?item.GetCooldown(＂mask＂) <= 0 
        equipR mask
        activate mask
     :?item.GetCooldown(＂bardiche＂)  <= 0 
     ^|(item.right = ＂bardiche＂
     ^& item.right.state = 2)
       equip bardiche
       activate R
     :
       equip repeating crossbow

    :? foe = ＂phase2＂
      ?foe.distance >= 7
         equip dashing shield
      :?item.GetCooldown(＂mask＂) <= 0 
         equipR mask
         activate mask
     :?item.GetCooldown(＂bardiche＂)  <= 0 
     ^|(item.right = ＂bardiche＂
     ^& item.right.state = 2)
       equip bardiche
       activate R

      :?foe = ＂fire＂
          equip Ether Staff
      :?foe = ＂ether＂
          equipL Vigor Sword
          equipR Fire Sword
      :?foe = ＂ice＂
          equipL Vigor Sword
          equipR Fire Sword
      :?foe = ＂vigor＂
          equipL Vigor Sword  
          equipR Poison Hammer
      :?foe = ＂poison＂
          equip Ice Staff

    :? foe = ＂phase3＂
      ?foe.distance >= 10
        equip dashing shield

     :? foe.state = 115 
      ^& foe.time >= 70
      ^&item.GetCooldown(＂mind＂) <= 0 
        equipL mind Stone

      :? foe.buffs.string = ＂fire＂
        equipR vigor sword
        equipL poison hammer 

      :? foe.buffs.string = ＂vigor＂
        ? hp > 45
          equip repeating crossbow
        : 
          equipR vigor sword
          equipL fire sword
     :
       equipL vigor sword 
       equipR fire sword

    :
      equipL Tri
      equip Dashing Shield

/*
    ? harvest.distance < 5
      equip shovel
      activate shovel 
*/
