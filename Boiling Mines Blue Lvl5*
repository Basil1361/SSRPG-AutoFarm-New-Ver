func BoilingMineLvl5()
  ? loc = Boiling Mine   
    ? summon.count = 0 & foe.count = 0
    ^& item.GetCooldown(＂cinderwisp＂) <= 0
    ^& item.CanActivate()
      equipL fire_talisman
      activate L

    :? foe.count >= 0 
    ^& pickup.distance <= 10
      equipL Star
      equipR Tri

    :? foe = Mine Walker  | foe = Dominotaur
    ^| foe = fire elemental | foe = Flame Geyser
    ^| foe = Whip Controller 
      ? foe.count >= 16
      ^&item.GetCooldown(＂blade＂) <= 0
        equip blade 
        activate blade
      ? foe.distance <= 25
        equip repeating
      ? foe.count > 0 & foe.distance <= 35
        activate potion
      :
        equip dashing shield
        equipL tri

   :? foe = ＂Bronze Guardian＂
     ?foe.state = 2 | foe.state = 1
       equip = repeating  crossbow

     :?foe.state = 32
       ^&item.GetCooldown(＂mind＂) <= 0 
       ^&foe.time >= 17
         equipL mind Stone

     :?foe.state = 33
       ?foe.distance >= 7
         equipL tri
         equip dashing shield
       :?item.GetCooldown(＂mask＂) <= 0 
         equipR mask
         activate mask
       :?item.GetCooldown(＂bardiche＂)  <= 0 
       ^|(item.right = ＂bardiche＂
       ^& item.right.state = 2)
         equip bardiche
         activate R
       :
         equipL vigor sword *7 + 13
         equipR poison hammer * 8 + 11
    : 
      equip dashing shield
      equipL tri
